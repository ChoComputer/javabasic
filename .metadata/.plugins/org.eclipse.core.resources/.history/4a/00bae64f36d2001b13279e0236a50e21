package structure;

public class CatStructure01 {
	
	public static void catInfo(Cat z) {
		
		System.out.println(z.Cname);
		System.out.println(z.Cage);
		System.out.println(z.Ctype);
		
			
		
	}
		
	
	public static void main(String[] args) {
		
		Cat a= new Cat();
		
		a.Cname="냥냥이";
		a.Cage=3;
		a.Ctype="bbbt";
				
		Cat b= new Cat();
		
		b.Cname="골골이";
		b.Cage=1;
		b.Ctype="aaa";	
		
				catInfo(a);
				catInfo(b);
				
				
		
		
	}

}
