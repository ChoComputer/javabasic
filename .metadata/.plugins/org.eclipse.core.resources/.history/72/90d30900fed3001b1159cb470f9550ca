package date210623.method;

public class Test {

	private int height;
	private int weight;

	
	public Test() {

		this.height = 175;
		this.weight = 64;
	}

	 // setter
	public Test(int h,int w) {
		this.height =h;
		this.weight=w;
	}

	// getter
	public double sosoBMI() {

		return (this.weight / (this.height * 0.01*this.height * 0.01));

	}
    // setter
	public void getBMI() {
		
		System.out.println(this.weight/(this.height*0.01*this.height * 0.01));
		// 그냥 h w로 하면안된다.
	}

}
