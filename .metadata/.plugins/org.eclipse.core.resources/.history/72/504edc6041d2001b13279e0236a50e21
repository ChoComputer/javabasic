package Quiz;

public class Dog {
	
	public String name;
	public int age;
	public String type;
	public boolean eat;
	
	public void getInfo() {
		System.out.println("멍뭉이 이름: "+name);
		System.out.println("멍뭉이 나이: "+age);
		System.out.println("멍뭉이 종: "+type);
		System.out.println("멍뭉이 밥먹음?: "+eat%n);
		
		
	}

}
