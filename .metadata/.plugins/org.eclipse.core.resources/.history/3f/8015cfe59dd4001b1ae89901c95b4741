package buyandsell;

public class Seller extends Buyer{
	
	//보유 최대 망고수
	//보유금액
	// buyer의 망고수
	// buyer의 금액
	



	private int storeMango;
	private int storeMoney;
	
	public void Seller(int sMg) {
		this.storeMango=sMg;
		this.storeMoney=0;
	}
	public void SellMango() {
		
	}
	
	
	
	public void showSeller() {
		System.out.println("---------------------------");
		System.out.println("상점 보유 망고 수 : "+storeMango+"원");
		System.out.println("상점 총 금액 : "+storeMoney+"원");
		System.out.println("---------------------------");
	}
	
	
	
	


}
