package buyandsell;

public class Main1 {
	public static void main(String[] args) {

		Buyer b = new Buyer(300000);
		Seller s=new Seller(1000);
		Seller s1=new Seller(3);
		
		
		
		s.showSeller();
		b.ShowBuyer();
		b.BuyMango(300, s);
		b.ShowBuyer();
		s.showSeller();
		s.SellMango(5);
		s.SellMango(2000);
		s.showSeller();
		b.BuyMango(300, s);
		s.showSeller();

	}
}
