package accessmodifier.game;

public class ProtectedCommoner {

	protected int hp;
	protected int level;
	protected int mp;
	protected int exp;

	public ProtectedCommoner() {
		this.hp = 20;
		this.mp = 10;
		this.level = 1;
		this.exp = 0;
	}

	public void ProtecdtedCommoner(int level, int exp, int hp, int mp) {

		this.level = level;
		this.exp = exp;
		this.hp = hp;
		this.mp = mp;
	}

}
