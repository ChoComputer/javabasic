package date210623.method;

public class Test {

	private int height;
	private int weight;

	// 생산자 getter setter 상관없음 
	// setter은 어차피 변수를 넣을거고 그렇게 되서 값이 변할꺼니까 생산자는 배경이라 생각하자
	// setter은 휘발성으로 값을 넣고 계산끝나면 날라감
	public Test() {

		this.height = 180;
		this.weight = 80;
	}

	 
	
	// getter
	public double getterBMI() {

		return (this.weight / (this.height * 0.01*this.height * 0.01));

	}
    // setter
	public void setterBMI(int h,int w) {
		this.height =h;
		this.weight=w;
		System.out.println(this.weight/(this.height*0.01*this.height * 0.01));
		// 그냥 h w로 하면안된다.
	}

}
