package day0522.operator;

public class Test04 {
	public static void main(String []args) {
	
		/*
		int a =5;
		int b = ++a;
		 *  ++이 변수이름 앞에 있을때
		 *  a= a+1;  <- 이것이 먼저 실행됨 인식됨
		 *  int b = a;
		 *  
		int a =5;
		int b = a++;

		 *  ++이 변수 이름 뒤에 있을때
		 *  int b = a; <-이게 먼저 실행된다
		 *  a= a+1;
		 */
		{
		int a =5;
		int b = ++a;
		System.out.println(a);
		System.out.println(b);
	}
		
	{	
		int a =5;
		int b = a++;
		System.out.println(a);
		System.out.println(b);
		
	}	
	
	{
		int a =5;
		System.out.println(a++);
		
		
	}
		
		
		
	}

}
