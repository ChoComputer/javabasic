package date210623.method;

public class Test {

	private int height;
	private int weight;
	
	
	//타입1
	public Test() {
		
		this.height=175;
		this.weight=64;
	}
	
	
	// 타입2
//	public Test(int h,int w) {
//		this.height =h;
//		this.weight=w;
