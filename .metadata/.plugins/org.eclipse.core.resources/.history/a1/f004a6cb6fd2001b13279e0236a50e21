package heapmemory;

public class ArrayComparison {
	public static void main(String[] args) {
		int[] arr1 = {1,2,3,4,5};
		int[] arr2 = arr1;
		int[] arr3 = {1,2,3,4,5};
		arr1[0] =10; //문자랑 다르게 일부 수정가능
		             // arr1 과 arr2는 동기화가 되어 주소 숫자가 다같다.
		System.out.println(arr1[0]);
		System.out.println(arr2[0]);
		System.out.println(arr3[0]);
				
		
	}

}
