package interaction.game.t;

public class Goul {
	private int hp;
	private int atk;
	private int def;
	
	
	public Goul() {
		this.hp=8;
		this.atk=2;
		this.def=1;
	}
	
	public void dobattle(int uatk) {
		this.hp=this.hp+this.def-uatk;
		if(this.hp <=0) {
			System.out.println("구울이 이미 죽었습니다.");
		}
		
	}

}
