package accessmodifier.game;

public class Main1 {
	public static void main(String[] args) {
		Warrior w=new Warrior("칼있수마훈");
		w.getInfo();
		w.doubleAttack();
		w.hunt();
		w.getInfo();
		w.doubleAttack();
		w.getInfo();
		System.out.println("레벨: " + level);
		System.out.println("체력 : " + hp);
		System.out.println("마력 : " + mp);
		System.out.println("경험치 : " + exp);
		System.out.println("----------------------");
	}

}
