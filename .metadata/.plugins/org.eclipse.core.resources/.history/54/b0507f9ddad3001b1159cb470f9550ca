package accessmodifier.game;

public class Commoner {
	
	protected int hp;
	protected int level;
	protected int mp;
	protected int exp;
	
	public void getInfo() {
		System.out.println("레벨: "+level);
		System.out.println("체력/마력: "+hp+"/"+mp);
		System.out.println("----------------------");
		
	}
	
	 
	
	

}
