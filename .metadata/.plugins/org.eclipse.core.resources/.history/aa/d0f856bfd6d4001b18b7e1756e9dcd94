package interaction.game.t;

public class Main {

	public static void main(String[] args) {
		Warrior w1 = new Warrior();
		Orc o1 = new Orc();
		Troll t1 = new Troll();
		
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.huntOrc(o1);
		w1.showStatus();
		w1.huntTroll(t1);
		w1.huntTroll(t1);
		w1.huntTroll(t1);
		w1.huntTroll(t1);
		w1.showStatus();
	}

}
