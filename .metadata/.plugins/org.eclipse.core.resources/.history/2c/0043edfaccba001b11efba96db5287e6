package day0522.operator;

public class Quiz03 {
	public static void main(String[] args) {
		
		String name ="아이유";
		int age =29;
		// 올해 마지막 20대인 아이유는 5억을 기부했다.
		//+연산자를 이용하여 출력 : println
	
		
		System.out.println("올해 마지막 20대인 %d살 %d는 5억을 기부했다",age, name );
		System.out.println();
		System.out.println();
		System.out.println();
	}

}
