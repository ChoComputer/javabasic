package accessmodifier2;

public class Main {
	public static void main(String[] args) {
		
		// Parent 를 상속받은 Child 내부의 자료 조회
		Child child =new Child();
		
		//1.public 자료
		System.out.println(child.a);
		
		//2. protected 자료
		// 현재 Main.java와 같은 패키지 내부에 Parent, Child
		// 가 존재하므로 protected 접근가능
		// 자식 부모 가능
		System.out.println(child.b);
		
		//3. private 자료
		// 같은 소속이어야 가능함  가장 까다로움
		// new Parent 소속임 new Child 소속이아님 Parent내부에 선언되있음!!
		System.out.println(child.c);
		
		//4. default 자료
		System.out.println(child.d);
		
		// Child에게 a~d 보고싶다고 요청하고 Child에서 판단함 
	}

}
