package javaStructure;

public class ClassMain {
	public static void main(String[] args) {
		// personclass가 먼저만들고
		// Person 2명만들기

		PersonClass person1 = new PersonClass();
		PersonClass person2 = new PersonClass();
		// 정보 기입
		
		person1.name="골골";
		person1.age=3;
		person1.pNum="010-000-0000";
		person1.address="갈갈국";
		person1.glasses= true;
 
		person2.name="억억억";
		person2.age=7;
		person2.pNum="000-1110-0111";
		person2.address="냑냑국";
		person2.glasses= false;
		//정보를 조회하면
		
		person1.getInfo(person1);
		person2.getInfo(person2);
		
	}

}
