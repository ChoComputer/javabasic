package heapmemory;

public class ArrayComparison {
	public static void main(String[] args) {
		int[] arr1 = {1,2,3,4,5};
		int[] arr2 = arr1;
		int[] arr3 = {1,2,3,4,5};
		arr1[0] =10; //문자랑 다르게 일부 수정가능
		             // 숫자는 문자와 다르게 주소가 같다
		System.out.println(arr1[0]);
		System.out.println(arr2[0]);
		System.out.println(arr3[0]);
				
		
	}

}
